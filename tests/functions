sqrt(4)       # 2.
sqrt(2)       # 1.414...
sqrt(4 * 4)   # 4.
sqrt(16 / 4)  # 4.
sqrt(3 ^ 12)  # 729.

facto(3)      # 6
facto(12)     # 479001600
facto(0)      # 0

fibo(0)       # 0
fibo(3)       # 2

max(1, 2)     # 2
max(1.3, 45)  # 45.

min(2, 1)     # 1
min(45.6, 2)  # 2.

isprime(3)    # 1
isprime(1)    # 0
isprime(2)    # 1

gcd(4, 6)     # 2
gcd(-4, -6)   # -2
gcd(10.2, 15) # 5
