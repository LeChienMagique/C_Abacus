# +
34 + 35                  # 69
(18 + 2) + 400           # 420
2.4 + 2.6                # 5.
2. + 3                   # 5.
-2 + 3                   # -1
-2. + 2                  # 0

# -
540 - 120                # 420
-130 - (-130)            # 0
12. - 2                  # 10.
12.3 - 0.3               # 12.
2.5 - 2.5                # 0

# *
-2 * 43                  # -66
12 * 5 * (-2)            # -120
2.5 * 2                  # 5.
0. * 333                 # 0

# /
12 / 2                   # 6
3 / 2                    # 1
3. / 2                   # 1.5
3 / 2.                   # 1.5

# ^
2 ^ 3                    # 8
-2 ^ 2                   # -4
2.2 ^ 2                  # 4.8
2 ^ 2.2                  # 4.59...

# %
5 % 2                    # 1
333 % 3                  # 0
3 % (-2)                 # -1
-3 % 2                   # -1 idk google says it's 1